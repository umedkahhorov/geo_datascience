   "source": [
    "a,x,c,y,n, b = symbols('a x c y n b')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "id": "8ac1bba9",
   "metadata": {},
   "outputs": [],
   "source": [
    "f = (1/n)*(y - (a*x + b))**2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "id": "16edb770",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\displaystyle \\frac{2 a x + 2 b - 2 y}{n}$"
      ],
      "text/plain": [
       "(2*a*x + 2*b - 2*y)/n"
      ]
     },
     "execution_count": 32,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "diff(f,b)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "id": "fe181e9e",
   "metadata": {},
   "outputs": [],
   "source": [
